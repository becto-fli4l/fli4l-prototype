hwmon_pcengines_wrap_temp ()
{
    var1=`sensors -u lm77-i2c-0-48 | grep temp1_input | cut -d : -f 2`
    var1=`echo ${var1}| sed 's/[^0-9]*\([0-9]*\)\.\([0-9]\{1\}\).*/\1.\2/'`

    show_tab_header ${_HWSUPP_RT_TEMP} no

    echo "<table class='normtable'>"
    echo "<tr>"
    echo "<th>${_HWSUPP_HWMON_SENSOR}</th>"
    echo "<th>${_HWSUPP_RT_TEMP}</th>"
    echo "</tr>"
    echo "<tr align='center'>"
    echo "<td>${_HWSUPP_RC_CPU}</td>"
    echo "<td>${var1}</td>"
    echo "</tr>"
    echo "</table>"

    show_tab_footer
}

hwmon_pcengines_wrap ()
{
    hwmon_pcengines_wrap_temp
}

