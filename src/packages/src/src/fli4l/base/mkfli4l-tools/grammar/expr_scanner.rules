\(                      { return LBRACKET; }
\)                      { return RBRACKET; }
==                      { return EQUAL; }
=~                      { return MATCH; }
!=                      { return UNEQUAL; }
&&                      { return AND; }
\|\|                    { return OR; }
!                       { return NOT; }
{NUMBER}                {
                            yylval->number = number_create(atoi(yytext), TRUE);
                            return NUMBER;
                        }
